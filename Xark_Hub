Race:Toggle("Auto Acient Quest",false, function(t)
    _G.Auto_Acient_Quest = t
end)

spawn(function()
    while wait() do 
        if _G.Auto_Acient_Quest then
            pcall(function()
                if game:GetService("Workspace").Enemies:FindFirstChild("Cocoa Warrior") or game:GetService("Workspace").Enemies:FindFirstChild("Chocolate Bar Battler") or game:GetService("Workspace").Enemies:FindFirstChild("Sweet Thief") or game:GetService("Workspace").Enemies:FindFirstChild("Candy Rebel") then
                    for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if v.Name == "Cocoa Warrior" or v.Name == "Chocolate Bar Battler" or v.Name == "Sweet Thief" or v.Name == "Candy Rebel" then
                           if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                               repeat task.wait()
                                    AutoHaki()
                                    Click()
                                    EquipWeapon(SelectWeapon)
                                    BringAcient = true
                                    v.HumanoidRootPart.CanCollide = false
                                    v.Humanoid.WalkSpeed = 0
                                    v.Head.CanCollide = false 
                                    AcientPos = v.HumanoidRootPart.CFrame
                                    TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,0))
                                until not _G.Auto_Acient_Quest or not v.Parent or v.Humanoid.Health <= 0
                                BringAcient = false
                            end
                        end
                    end
                else         
                    for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do 
                        if v.Name == "Cocoa Warrior" then
                            TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,0))
                        elseif v.Name == "Chocolate Bar Battler" then
                            TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,0))
                        elseif v.Name == "Sweet Thief" then
                            TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,0))
                        elseif v.Name == "Candy Rebel" then
                            TP(v.HumanoidRootPart.CFrame * CFrame.new(0,50,0))
                        end
                    end
                end
            end)
        end
    end
end)

spawn(function()
    pcall(function()
        while wait() do
            if _G.Auto_Acient_Quest then
                if game.Players.LocalPlayer.Character.RaceTransformed.Value == false then
                    _G.Auto_Acient_Quest = true
                end
            end
        end
    end)
end)

spawn(function()
    while wait() do
        pcall(function()
            if _G.Auto_Acient_Quest then
                game:GetService("VirtualInputManager"):SendKeyEvent(true,"Y",false,game)
                wait(0.1)
                game:GetService("VirtualInputManager"):SendKeyEvent(false,"Y",false,game)
            end
        end)
    end
end)
